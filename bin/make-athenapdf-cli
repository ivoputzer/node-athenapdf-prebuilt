#!/usr/bin/env sh

set -o xtrace

git config -f .gitmodules --get-regex submodule.*url \
| sed 's/submodule.//g;s/.url//g' \
| awk '{print $2,$1}' \
| xargs -n 2 git clone --progress --verbose --recursive \
| sh

npm install lib/athenapdf/cli

npm explore athenapdf -- npm install
npm explore athenapdf -- npm run build
